{
  "name": "viction-extension",
  "displayName": "Viction Wallet",
  "version": "1.1.4",
  "description": "Viction Wallet by Coin98 ",
  "author": "Coin98",
  "private": true,
  "scripts": {
    "dev": "plasmo dev",
    "build": "plasmo build",
    "build:firefox": "plasmo build --target=firefox-mv3",
    "package": "plasmo package",
    "format": "prettier --write .",
    "prettier:check": "prettier --check ."
  },
  "dependencies": {
    "@headlessui/react": "1.6.4",
    "@loadable/component": "^5.15.3",
    "@plasmohq/redux-persist": "^6.1.0",
    "@plasmohq/storage": "^0.12.2",
    "@reduxjs/toolkit": "^1.9.5",
    "@tanstack/react-query": "^4.29.3",
    "@types/bs58": "^4.0.1",
    "@vespaiach/axios-fetch-adapter": "^0.3.1",
    "@wallet/base": "3.0.4",
    "@wallet/constants": "3.0.4",
    "@wallet/core": "3.0.4",
    "@wallet/evm": "3.0.4",
    "@wallet/extension-stream": "workspace:*",
    "@wallet/screen": "workspace:*",
    "@wallet/ui": "workspace:*",
    "@wallet/utils": "3.0.5",
    "animate.css": "^4.1.1",
    "async": "^3.2.4",
    "axios": "0.21.1",
    "bech32": "^1.1.4",
    "bignumber.js": "^9.1.1",
    "bip39": "3.0.2",
    "classnames": "^2.3.2",
    "config-custom": "workspace:*",
    "crypto-js": "^4.1.1",
    "dayjs": "^1.11.7",
    "elliptic": "^6.5.4",
    "ethers": "4.0.46",
    "i18n": "workspace:*",
    "i18next": "^22.4.14",
    "lodash": "^4.17.21",
    "plasmo": "0.73.2",
    "query-string": "^8.1.0",
    "react": "18.0.0",
    "react-countup": "^6.4.2",
    "react-dom": "18.0.0",
    "react-error-boundary": "^4.0.4",
    "react-hook-form": "^7.43.9",
    "react-i18next": "^11.18.6",
    "react-infinite-scroll-component": "^6.1.0",
    "react-lottie-player": "^1.5.4",
    "react-qr-code": "^2.0.11",
    "react-redux": "^8.0.5",
    "react-router-dom": "^5.2.0",
    "react-spinners": "^0.13.8",
    "react-toastify": "^9.1.2",
    "react-transition-group": "^4.4.5",
    "react-use": "^17.4.0",
    "redux": "^4.2.1",
    "redux-persist-webextension-storage": "^1.0.2",
    "secp256k1": "^5.0.0",
    "store": "workspace:*",
    "tailwind-config": "workspace:*",
    "web3": "^1.10.3"
  },
  "devDependencies": {
    "@plasmohq/prettier-plugin-sort-imports": "3.6.4",
    "@types/chrome": "0.0.228",
    "@types/crypto-js": "^4.1.1",
    "@types/lodash": "^4.14.187",
    "@types/node": "18.15.11",
    "@types/react": "18.0.33",
    "@types/react-dom": "18.0.11",
    "@types/react-router-dom": "^5.2.0",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.0.1",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.32.2",
    "postcss": "^8.2.6",
    "postcss-nested": "^5.0.6",
    "prettier": "2.8.7",
    "tailwindcss": "^3.3.1",
    "typescript": "5.0.4"
  },
  "manifest": {
    "default_locale": "en",
    "browser_specific_settings": {
      "gecko": {
        "id": "ramper@coin98.finance"
      }
    },
    "permissions": [
      "activeTab",
      "tabs",
      "unlimitedStorage",
      "clipboardRead"
    ],
    "content_security_policy": {
      "extension_pages": "script-src 'self' 'wasm-unsafe-eval'; object-src 'self'"
    },
    "web_accessible_resources": [
      {
        "resources": [
          "public/**"
        ],
        "matches": [
          "*://*/*"
        ]
      }
    ],
    "host_permissions": [
      "*://*/"
    ]
  }
}
